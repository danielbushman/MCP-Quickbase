{
  "name": "quickbase-mcp-connector",
  "version": "2.0.0",
  "description": "Quickbase connector for Model Context Protocol",
  "main": "dist/mcp-stdio-server.js",
  "types": "dist/mcp-stdio-server.d.ts",
  "bin": {
    "quickbase-mcp": "./dist/mcp-stdio-server.js"
  },
  "scripts": {
    "build": "tsc",
    "prestart": "./killProcess.sh",
    "start": "node dist/mcp-stdio-server.js",
    "start:http": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build && npm test"
  },
  "author": "Quickbase MCP Connector Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielbushman/Quickbase-MCP-connector.git"
  },
  "homepage": "https://github.com/danielbushman/Quickbase-MCP-connector#readme",
  "bugs": {
    "url": "https://github.com/danielbushman/Quickbase-MCP-connector/issues"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "quickbase",
    "claude",
    "ai",
    "database",
    "construction",
    "project-management",
    "workflow",
    "automation"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.2",
    "@types/cors": "^2.8.18",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
